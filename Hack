end
end)


Section:NewToggle("Turn Off Egg Animation", "", function(state)
  if state then
   game.Players.LocalPlayer.PlayerGui["EggOpenGui"].Enabled = false
  else
   game.Players.LocalPlayer.PlayerGui["EggOpenGui"].Enabled = true
end
 end)


